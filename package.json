{
  "name": "react-vstorie",
  "description": "An easy to use React component for stories instagram like",
  "author": "Vinicius Cavalcanti Santos",
  "version": "0.0.4",
  "private": false,
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vstorie",
    "react",
    "stories",
    "instagram",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "path": "^0.12.7",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViniciusCavalcanteSantos/vstorie.git"
  },
  "bugs": {
    "url": "https://github.com/ViniciusCavalcanteSantos/vstorie/issues"
  },
  "homepage": "https://github.com/ViniciusCavalcanteSantos/vstorie#readme",
  "license": "MIT"
}
